    "@types/cookie": "^0.5.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "type": "module",
  "pnpm": {
    "overrides": {
      "@sveltejs/kit": "kit/packages/kit"
    }
